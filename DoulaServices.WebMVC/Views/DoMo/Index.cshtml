@model IEnumerable<DoulaServices.Models.DoMo.ListItem>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DoMoId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DoulaName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Notes)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DoMoId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DoulaName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Notes)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.DoMoId }) |
            @Html.ActionLink("Details", "Details", new { id = item.DoMoId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.DoMoId })
        </td>
    </tr>
}

</table>
